#audio
	display none
#block1
	img
		position relative
		cursor pointer
		width 15%
		z-index 3
#opc
	padding 0
	margin 2em 0 0 0
	li
		cursor pointer
		list-style none
		margin 1em 0
		text-align center
		width 100%
	li:hover
		color orange
		text-shadow 1px 1px 1px #333
#preg
	height 40px
	text-align center
	width 100%
.h
	display inline-block
	vertical-align top
	width 70%
	div
		position relative
	div:not(.line-3)
		text-align center
		width 100%;
	.line-3 img:last-child
		float right
	.line-2
		bottom 20px
	.line-3
		bottom 70px
	.line-4
		bottom 120px
.h:last-child
	background url('img/opc.jpg')
	background-repeat no-repeat
	height 300px
	background-size 100%
	width 25% 